{
  "name": "pseudosense",
  "displayName": "pseudosense",
  "description": "intellisense for pseudocode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "pseudosense",
        "aliases": [
          "pseudosense"
        ],
        "extensions": [
          ".pseudo",
          ".aqa"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "snippets": [
      {
          "language": "pseudosense",
          "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
