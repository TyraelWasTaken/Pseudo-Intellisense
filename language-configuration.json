{
    "comments": {
      "lineComment": "//",
      "blockComment": ["/*", "*/"]
    },
    "brackets": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["RECORD", "ENDRECORD"]
    ],
    "autoClosingPairs": [
      { "open": "{", "close": "}" },
      { "open": "[", "close": "]" },
      { "open": "(", "close": ")" },
      { "open": "'", "close": "'", "notIn": ["string", "comment"] },
      { "open": "\"", "close": "\"", "notIn": ["string"] },
      { "open": "`", "close": "`", "notIn": ["string", "comment"] },
      { "open": "/**", "close": " */", "notIn": ["string"] },
      { "open": "RECORD", "close": "ENDRECORD" }
      
    ],
    "autoCloseBefore": ";:.,=}])>` \n\t",
    "surroundingPairs": [
      ["{", "}"],
      ["[", "]"],
      ["(", ")"],
      ["'", "'"],
      ["\"", "\""],
      ["`", "`"],
      ["RECORD", "ENDRECORD"]
    ],
    "folding": {
      "markers": {
        "start": "^\\s*//\\s*#?region\\b",
        "end": "^\\s*//\\s*#?endregion\\b"
      }
    }
  }